        <flow-ref name="get:/countries:api-config" doc:name="Flow-ref to get:/countries:api-config"/>
    </munit:test>
    <munit:test name="wp-demo02-test-suite-get:/cities/{countryName}:api-configTest" description="Test">
        <flow-ref name="get:/cities/{countryName}:api-config" doc:name="Flow-ref to get:/cities/{countryName}:api-config"/>
    </munit:test>
    <munit:test name="wp-demo02-test-suite-get:/countries/{countryName}:api-configTest" description="Test">
        <flow-ref name="get:/countries/{countryName}:api-config" doc:name="Flow-ref to get:/countries/{countryName}:api-config"/>
